{
    "name": "Simple Extension",
    "manifest_version": 2,
    "description": "Hello World :) Just laying out a simple Structure!",
    "icons": { "128": "icon.png" },
    "version": "1.0",
    "background": {
        "persistent": false,
        "scripts": ["jquery.min.js","background.js"]
    },
    "permissions": [
        "tabs", "<all_urls>"
      ],
    
    "browser_action": {
        "default_title": "Have a good day",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}